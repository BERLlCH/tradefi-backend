{
  "name": "daosign-backend",
  "version": "1.0.3",
  "description": "Backend server for DaoSign",
  "scripts": {
    "test": "npm exec jest ./src",
    "lint": "npm run prettier-check && npm run eslint-check",
    "prettier-format": "npm exec -- prettier --write .",
    "prettier-check": "npm exec -- prettier --check .",
    "eslint-check": "npm exec eslint --ext=ts,tsx src",
    "build": "npm exec tsc",
    "postbuild": "yarn copy-files",
    "dev": "npm exec ts-node ./src/server.ts",
    "nodemon": "nodemon -w src --ext ts --exec ts-node ./src/server.ts",
    "copy-files": "copyfiles -u 1 **/src/**/*.html **/src/**/*.css ./dist/",
    "start": "NODE_ENV=production npm exec node ./dist/server.js",
    "prod": "npm run build && npm run start",
    "migration:create": "cd src/migrations && npx typeorm-ts-node-commonjs migration:create",
    "migrate:up": "npx typeorm-ts-node-commonjs migration:run -d src/ormconfig.ts",
    "migrate:down": "npx typeorm-ts-node-commonjs migration:revert -d src/ormconfig.ts",
    "submodule": "git submodule update --init"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ConsiderItDone/daosign-backend.git"
  },
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@aws-sdk/client-s3": "^3.321.1",
    "@aws-sdk/lib-storage": "^3.321.1",
    "@mailchimp/mailchimp_transactional": "^1.0.50",
    "@pinata/sdk": "^2.1.0",
    "@types/html-pdf": "^3.0.0",
    "apollo-server-core": "^3.8.2",
    "apollo-server-express": "^3.8.2",
    "axios": "^1.3.6",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "cp": "^0.2.0",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.1",
    "envalid": "^7.3.1",
    "ethers": "^5.7.2",
    "express": "^4.18.1",
    "formidable": "^2.0.1",
    "fs": "^0.0.1-security",
    "graphql": "^15.8.0",
    "html-pdf": "^3.0.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "md-to-pdf": "^5.2.4",
    "nodemailer": "^6.9.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "openai": "^3.2.1",
    "pdf-creator-node": "^2.3.5",
    "pdf-lib": "^1.17.1",
    "postinstall-postinstall": "^2.1.0",
    "puppeteer": "^20.1.2",
    "puppeteer-core": "^20.1.2",
    "request-ip": "^2.2.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.6",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/formidable": "^2.0.5",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mailchimp__mailchimp_transactional": "^1.0.5",
    "@types/node": "^17.0.38",
    "@types/puppeteer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.16",
    "prettier": "2.6.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.8.0",
    "typeorm-model-generator": "^0.4.6",
    "typescript": "^4.7.2"
  }
}
